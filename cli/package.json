  "repository": {
    "type": "git",
    "url": "https://github.com/mukarrammahmoud/react-assets-gen.git",
    "directory": "cli"
  },
  "bugs": {
    "url": "https://github.com/mukarrammahmoud/react-assets-gen/issues"
  },
  "homepage": "https://github.com/mukarrammahmoud/react-assets-gen#readme",
  "author": "Mukarram Mahmoud",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "commander": "^12.1.0",
    "picocolors": "^1.1.1"
