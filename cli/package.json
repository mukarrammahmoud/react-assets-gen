{
  "name": "react-assets-gen",
  "version": "1.0.11",
  "description": "Production-ready React Assets Code Generator - Type-safe asset imports for React projects",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "assets-gen": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
